<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unq.gatoencerrado_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LaberintosListActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LaberintoDetailActivity"
            android:label="@string/title_laberinto_detail"
            android:parentActivityName=".LaberintosListActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LaberintosListActivity" />
        </activity>

        <activity
            android:name=".InventarioActivity"
            android:label="@string/title_inventario"
            android:parentActivityName=".InventarioActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LaberintosListActivity" />
        </activity>

    </application>

</manifest>