<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="ciu.unq.laberintoapplication">

<uses-permission android:name="android.permission.INTERNET"/>

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity
        android:name="ciu.unq.laberintoapplication.DisplayMessageActivity"
        android:label="@string/title_activity_display_message"
        android:parentActivityName="ciu.unq.laberintoapplication.MainActivity" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="ciu.unq.laberintoapplication.MainActivity" />
    </activity>

</application>

</manifest>
